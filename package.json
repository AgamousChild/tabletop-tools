{
  "name": "tabletop-tools",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel --filter nocheat-server --filter nocheat-client dev",
    "build": "pnpm --filter nocheat-client build",
    "test": "pnpm -r test",
    "lint": "eslint apps/nocheat/server/src apps/nocheat/client/src packages --ext .ts,.tsx",
    "lint:fix": "eslint apps/nocheat/server/src apps/nocheat/client/src packages --ext .ts,.tsx --fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.8.1",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
